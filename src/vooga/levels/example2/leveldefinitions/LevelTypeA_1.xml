<level>
	<collision_manager g1=red g2=blue class=vooga.levels.example2.collisions.ExampleCollisionManager />
	
	<sprite group=red class=vooga.levels.example2.sprites.RedSprite name=redEnemy>
		<x>100</x>
	</sprite>
	
	<sprite group=blue class=vooga.levels.example2.sprites.BlueSprite name=blueEnemy>
		<y>300</y>
	</sprite>
	
	<instance type=redEnemy>
		<y>100</y>
		<fast>true</fast>
	</instance>
	
	<instance type=redEnemy>
		<y>200</y>
		<fast>false</fast>
	</instance>
